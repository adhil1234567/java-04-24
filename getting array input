import java.util.Scanner;
class main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int a[]=new int[3];
        int i,n=0;
        for(i=0;i<a.length;i++){
            System.out.println("enter "+ ++n +" value ;");
            a[i]=sc.nextInt();
        }
        System.out.println("the result is");
        for(i=0;i<a.length;i++){
            System.out.println(a[i]);
        }
    }
}
