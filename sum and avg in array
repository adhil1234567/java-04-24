import java.util.Scanner;
class main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int a[]=new int[5];
        int i;
        int sum=0;
        double avg=0;
        for(i=0;i<a.length;i++){
        a[i]=sc.nextInt();
        }
        for(i=0;i<a.length;i++){
        sum=sum+a[i];
        }
        avg=sum/a.length;
        System.out.println("sum :"+sum);
        System.out.println("avg  :"+avg);
    }
}
